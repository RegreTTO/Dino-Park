<UserControl x:Class="Dino_Park_2._0.Octopus"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Dino_Park_2._0"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" x:Name="octopus" Unloaded="octopus_Unloaded" Loaded="octopus_Loaded">
    <Grid x:Name="grid">
        <Grid.RowDefinitions>
            <RowDefinition Height="76*"/>
            <RowDefinition Height="149*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="81*"/>
            <ColumnDefinition Width="119*"/>
        </Grid.ColumnDefinitions>
        <Rectangle Fill="Purple" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
    </Grid>
</UserControl>
